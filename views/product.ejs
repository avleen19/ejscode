<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>To-Do List</title>
</head>

<body>
    <%- include("./partials/header") %>



    <ol>
        <% products.forEach(function(product, index) { %>
            <li>
                
                <h4><%= product.name %></h4>
                <h4><%= product.price %></h4>
            </li>
        <% }); %>
    </ol>

</body>
</html>
